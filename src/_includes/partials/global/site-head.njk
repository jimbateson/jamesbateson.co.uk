<a class="skip-link" href="#main-content">Skip to content</a>
<header role="banner" class="[ site-head ] [ pad-bottom-100 ]">
	<div class="wrapper">
		<span class="visually-hidden">{{ site.name }}: {{ pageType }}</span>
		<div class="[ site-head__inner ]">
			<a href="/" class="[ site-head__site-name ] [ leading-tight ]"{{ 'aria-current=page' if page.url == '/' | safe }}>
				<span class="visually-hidden">{{ 'Back to homepage' if page.url != '/' else 'Homepage' }}</span>
				<svg class="site-logo" width="80" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 60 60"><defs><style>.cls2 {fill:none;stroke:#fff;stroke-linecap:round;stroke-linejoin:round;stroke-width:1.15px}</style></defs><path class="cls2" d="M40.52 28.4l-2.55 2.66-3.5-2.72-2.72 2.72-3.26-3.2M10.7 43.69l11.25-18.57 3.88 6.56"></path><path class="cls2" d="M18.9 43.69l15.44-25.48 15.45 25.48M25.83 43.69H6.29M52.94 43.69H29.77"></path></svg>
			</a>
			{% include "partials/components/nav.njk" %}
		</div>
	</div>
</header>
